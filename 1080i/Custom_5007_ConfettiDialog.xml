<?xml version="1.0" encoding="utf-8"?>
<!--Confetti Dialog-->
<window type="dialog" id="5007">
	<defaultcontrol always="true">450</defaultcontrol>
	<coordinates>
		<left>0</left>
		<top>-55</top>
		<system>1</system>
	</coordinates>
	<include>Animation_DialogPopup</include>
	<controls>
		<control type="image">
			<left>260</left>
			<top>150</top>
			<width>1400</width>
			<height>560</height>
			<texture colordiffuse="$VAR[ColorDiffuseVar]">frames/panel_back.png</texture>
		</control>
		<control type="image">
			<left>357</left>
			<top>180</top>
			<width>1212</width>
			<height>500</height>
			<texture colordiffuse="$VAR[ColorDiffuseVar]">osd/osd_settings_back_glow.png</texture>
			<visible>!Skin.HasSetting(DisableGlowbar)</visible>
			<include>PanelGlowFade</include>
		</control>
		<control type="image">
			<left>357</left>
			<top>180</top>
			<width>1212</width>
			<height>500</height>
			<texture colordiffuse="$VAR[DialogColorVar]">osd/osd_settings_back.png</texture>
		</control>
		<control type="image">
			<left>375</left>
			<top>212</top>
			<width>1172</width>
			<height>60</height>
			<texture border="1">osd/osd_settings_buttonnf.png</texture>
		</control>
		<control type="label">
			<left>820</left>
			<top>205</top>
			<width>600</width>
			<height>75</height>
			<font>Font_Bold30_Caps</font>
			<textcolor>white</textcolor>
			<label>[B]Confetti Settings[/B]</label>
			<font>Font_Bold30_Caps</font>
			<textcolor>white</textcolor>
		</control>
		<control type="image" >
				<left>800</left>
				<top>274</top>
				<width>725</width>
				<height>368</height>
				<texture>frames/frame-cadre.png</texture>
			</control>
		<control type="image" >
				<left>804</left>
				<top>279</top>
				<width>725</width>
				<height>358</height>
				<texture>holiday_additions/confetti/screenshot.png</texture>
			</control>
		<control type="grouplist" id="450">
			<left>375</left>
			<top>272</top>
			<width>1172</width>
			<height>540</height>
			<pagecontrol>60</pagecontrol>
			<onleft>60</onleft>
			<onright>60</onright>
			<onup>450</onup>
			<ondown>450</ondown>
			<control type="radiobutton" id="109">
				<label>Light Confetti</label>
				<include>RadioButtonShorter</include>
				<onclick>Skin.ToggleSetting(confettil)</onclick>
				<selected>Skin.HasSetting(confettil)</selected>
			</control>
			<control type="radiobutton" id="110">
				<label>Medium Confetti</label>
				<include>RadioButtonShorter</include>
				<onclick>Skin.ToggleSetting(confettim)</onclick>
				<selected>Skin.HasSetting(confettim)</selected>
			</control>
			<control type="radiobutton" id="111">
				<label>Heavy Confetti</label>
				<include>RadioButtonShorter</include>
				<onclick>Skin.ToggleSetting(confettih)</onclick>
				<selected>Skin.HasSetting(confettih)</selected>
			</control>
			<control type="radiobutton" id="112">
				<label>Falling Confetti</label>
				<include>RadioButtonShorter</include>
				<onclick>Skin.ToggleSetting(confettif)</onclick>
				<selected>Skin.HasSetting(confettif)</selected>
			</control>
			<control type="radiobutton" id="11">
				<label>Confetti Floor</label>
				<include>RadioButtonShorter</include>
				<onclick>Skin.ToggleSetting(confettifl)</onclick>
				<selected>Skin.HasSetting(confettifl)</selected>
			</control>
		</control>
		<control type="scrollbar" id="60">
			<include>Objects_CommonSettingsScrollbar</include>
			<onleft>450</onleft>
			<onright>450</onright>
			<ondown>60</ondown>
			<onup>60</onup>
		</control>
	</controls>
</window>



